@{
    ViewData["Title"] = "Confirmação";
}


<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="~/css/confirmacao.css" asp-append-version="true" />
</head>

<div class="page-container">
    <div class="page-title">
        SUCESSO
    </div>
    <div class="confirmation-image">
        <img src="~/imagens/checkmark.png" alt="Checkmark" />
    </div>
    <div class="report-id">
        <h1>ID:</h1>
        <div class="id-box">
            <p>@ViewBag.Id</p>
        </div>
    </div>
</div>
